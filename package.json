{
  "name": "@staypony/talker",
  "version": "0.1.0",
  "description": "Chatting platform by StayPony Team",
  "private": true,
  "license": "MIT",
  "repository": "git@github.com:staypony/talker.git",
  "author": {
    "name": "StayPony Team",
    "email": "contact@staypony.space",
    "url": "https://staypony.space"
  },
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "yarn lint:script",
    "lint:script": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "yarn format:script && yarn format:styl",
    "format:script": "prettier --write ./**/*.{js,ts,vue}",
    "format:styl": "stylus-supremacy format ./**/*.styl --replace --options ./stylus-supremacy.json",
    "typegen": "graphql-codegen",
    "posttypegen": "prettier --write ./graphql/**/*.ts",
    "predeploy": "yarn build",
    "deploy": "node ./scripts/deploy.js",
    "mc": "node ./scripts/make-component.js"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/svg-sprite": "^0.4.10",
    "apollo-link-error": "^1.1.13",
    "core-js": "^2.6.11",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.9.1",
    "nuxt-property-decorator": "^2.5.1",
    "universal-cookie": "^4.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuescroll": "^4.16.1",
    "vuex": "^3.1.3",
    "vuex-class": "^0.3.2",
    "vuex-class-component": "^2.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.3",
    "@graphql-codegen/schema-ast": "^1.17.3",
    "@graphql-codegen/typescript": "^1.17.3",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.17.3",
    "@graphql-codegen/typescript-operations": "^1.17.3",
    "@nuxt/types": "^2.13.3",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/router": "^1.5.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.3"
  }
}
